 => {\n    const {\n      createOrder: { ok, orderId },\n    } = data;\n    if (data.createOrder.ok) {\n      history.push(`/orders/${orderId}`);\n    }\n  };\n  const [createOrderMutation, { loading: placingOrder }] = useMutation<\n    createOrder,\n    createOrderVariables\n  >(CREATE_ORDER_MUTATION, {\n    onCompleted,\n  });\n  const triggerConfirmOrder = () => {\n    if (orderItems.length === 0) {\n      alert(\"Can't place empty order\");\n      return;\n    }\n    const ok = window.confirm(\"You are about to place an order\");\n    if (ok) {\n      createOrderMutation({\n        variables: {\n          input: {\n            restaurantId: +params.id,\n            items: orderItems,\n          },\n        },\n      });\n    }\n  };\n  return (\n    <div>\n      <div\n        className=\" bg-gray-800 py-48 bg-cover bg-top\"\n        style={{\n          backgroundImage: `url(${data?.restaurant.restaurant?.coverImg})`,\n        }}\n      >\n        <div className=\"bg-white w-3/12 py-8 pl-44\">\n          <h4 className=\"text-2xl mb-3\">{data?.restaurant.restaurant?.name}</h4>\n          <h5 className=\" text-sm font-light mb-2\">\n            {data?.restaurant.restaurant?.address}\n          </h5>\n        </div>\n      </div>\n      <div className=\"container pb-32 flex flex-col items-end mt-20\">\n        {!orderStarted && (\n          <button onClick={triggerStartOrder} className=\"btn px-10\">\n            Start Order\n          </button>\n        )}\n        {orderStarted && (\n          <div className=\"flex items-center\">\n            <button onClick={triggerConfirmOrder} className=\"btn px-10 mr-3\">\n              Confirm Order\n            </button>\n            <button\n              onClick={triggerCancelOrder}\n              className=\"btn px-10 bg-black hover:bg-black\"\n            >\n              Cancel Order\n            </button>\n          </div>\n        )}\n        <div className=\" w-full grid mt-16 md:grid-cols-3 gap-x-5 gap-y-10\">\n          {data?.restaurant.restaurant?.menu?.map((dish, index) => (\n            <Dish\n              isSelected={isSelected(dish.id)}\n              id={dish.id}\n              orderStarted={orderStarted}\n              key={index}\n              name={dish.name}\n              description={dish.description}\n              price={dish.price}\n              isCustomer={true}\n              options={dish.options}\n              addItemToOrder={addItemToOrder}\n              removeFromOrder={removeFromOrder}\n            >\n              {dish.options?.map((option, index) => (\n                <DishOption\n                  key={index}\n                  dishId={dish.id}\n                  isSelected={isOptionSelected(dish.id, option.name)}\n                  name={option.name}\n                  extra={option.extra}\n                  addOptionToItem={addOptionToItem}\n                  removeOptionFromItem={removeOptionFromItem}\n                ></DishOption>\n              ))}\n            </Dish>\n          ))}\n        </div>\n      </div>\n    </div>\n  );\n};\n","/Users/zizimoos/Desktop/third/cub-uber-frontend/src/components/app.tsx",[],"/Users/zizimoos/Desktop/third/cub-uber-frontend/src/pages/owner/my-restaurants.tsx",[],"/Users/zizimoos/Desktop/third/cub-uber-frontend/src/pages/owner/add-restaurants.tsx",[],"/Users/zizimoos/Desktop/third/cub-uber-frontend/src/pages/owner/my-restaurant.tsx",[],"/Users/zizimoos/Desktop/third/cub-uber-frontend/src/pages/owner/add-dish.tsx",[],"/Users/zizimoos/Desktop/third/cub-uber-frontend/src/components/dish.tsx",[],"/Users/zizimoos/Desktop/third/cub-uber-frontend/src/components/dish-option.tsx",[],"/Users/zizimoos/Desktop/third/cub-uber-frontend/src/__generated__/globalTypes.ts",[],{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":150,"column":22,"nodeType":"130","messageId":"131","endLine":150,"endColumn":24},{"ruleId":"128","severity":1,"message":"132